{
  "name": "muta-cli",
  "version": "0.2.0-dev.2",
  "author": "me@huwenchao.com",
  "bin": {
    "muta-cli": "./bin/run"
  },
  "bugs": "https://github.com/huwenchao/muta-cli/issues",
  "dependencies": {
    "@mutadev/muta-sdk": "0.2.0-dev.2",
    "@mutadev/service": "0.2.0-dev.2",
    "@mutadev/client": "0.2.0-dev.2",
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "@types/graphql": "^14.5.0",
    "graphql": "^15.2.0",
    "lodash": "^1.0.0",
    "node-repl-await": "^0.1.1",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "chai": "^4",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "globby": "^10",
    "mocha": "^5",
    "nyc": "^14",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/huwenchao/muta-cli",
  "keywords": [
    "oclif",
    "muta"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "muta-cli",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "huwenchao/muta-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md",
    "example": "node example/muta_sdk_demo.js"
  },
  "types": "lib/index.d.ts"
}
